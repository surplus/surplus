{
	"name": "@surplus/s",
	"version": "0.5.0",
	"description": "S.js - simple, clean, fast reactive programming in Javascript",
	"main": "dist/S.js",
	"module": "dist/es/S.js",
	"types": "dist/es/S.d.ts",
	"files": [
		"README.md",
		"LICENSE",
		"dist"
	],
	"devDependencies": {
		"jasmine": "3.4.0",
		"rollup": "1.16.2",
		"typescript": "5.3.3",
		"prettier": "3.2.4"
	},
	"scripts": {
		"build": "tsc && rollup dist/es/S.js -f \"umd\" -n \"S\" -o dist/S.js && rollup dist/es/withsubclocks.js -f \"umd\" -n \"S\" -o dist/withsubclocks.js",
		"prepublish": "npm run build",
		"test": "jasmine",
		"bench": "node --allow-natives-syntax bench/bench.js",
		"lint": "prettier --no-editorconfig --check . ",
		"format": "prettier --no-editorconfig --write ."
	},
	"keywords": [
		"reactive",
		"javascript",
		"synchronous",
		"signal",
		"dependency"
	],
	"repository": "surplus/s",
	"bugs": {
		"url": "https://github.com/surplus/s/issues"
	},
	"author": "Adam Haile",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	}
}
